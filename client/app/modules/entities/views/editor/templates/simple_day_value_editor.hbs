{{#if editMode}}
  <div class="edit-mode">
    <div class="unit-pickers has-alertbox">
      {{> 'entities:editor:time_unit_picker' yearData}}
      {{> 'entities:editor:time_unit_picker' monthData}}
      {{> 'entities:editor:time_unit_picker' dayData}}
    </div>
    {{> 'entities:editor:edit_mode_buttons' this}}
  </div>
{{else}}
  <div class="displayModeData">{{value}}</div>
  {{> 'entities:editor:edit_button'}}
{{/if}}
